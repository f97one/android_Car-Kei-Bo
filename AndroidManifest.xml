<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="net.formula97.andorid.car_kei_bo"
	android:versionCode="3"
	android:versionName="0.7-p2" android:installLocation="auto">

	<uses-sdk android:minSdkVersion="8" />

	<application
		android:icon="@drawable/ic_launcher"
		android:label="@string/app_name" >
		<activity android:name=".CarList" >
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".AddMyCar" >
			<intent-filter>
				<action android:name="android.intent.action.INSERT" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".Config" >
			<intent-filter>
				<action android:name="android.intent.action.EDIT" />

				<category android:name="android.intent.category.PREFERENCE" />
			</intent-filter>
		</activity>
		<activity android:name=".MileageList" >
			<intent-filter>
				<action android:name="android.intent.action.INSERT" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".FuelMileageAdd" >
			<intent-filter>
				<action android:name="android.intent.action.INSERT" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".AboutLicense">
			<intent-filter>
				<action android:name="android.intent.action.VIEW"/>
				<category android:name="android.intent.category.INFO"/>
			</intent-filter>
		</activity>
		<activity android:name=".ShowStats">
			<intent-filter>
				<action android:name="android.intent.action.VIEW"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
	</application>

</manifest>